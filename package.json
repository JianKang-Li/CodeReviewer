{
  "name": "codereviewer",
  "displayName": "codeReviewer",
  "description": "代码自检",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codereviewer.JkReview",
        "title": "JkReview"
      },
      {
        "command": "codereviewer.lint",
        "title": "JkLint"
      }
    ],
    "keybindings": [
      {
        "command": "codereviewer.JkReview",
        "key": "ctrl+r",
        "mac": "cmd+r",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "codereviewer.lint",
          "group": "JkReview",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}